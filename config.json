{
  "openai_api_key": null,
  "ontology_path": "Ontology_schema/ontology_schema.json",
  "templates_dir": "templates",
  "storage_dir": "cases/processed",
  "processing_settings": {
    "max_text_length": 15000,
    "confidence_threshold": 0.5,
    "enable_regex_fallback": true,
    "max_residual_facts": 10,
    "save_markdown_files": true,
    "markdown_output_dir": "cases/markdown"
  },
  "logging": {
    "level": "INFO",
    "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s",
    "log_to_file": false,
    "log_file_path": "logs/pipeline.log"
  },
  "extraction_settings": {
    "llm_model": "gpt-4o",
    "max_retries": 3,
    "timeout_seconds": 120
  }
}